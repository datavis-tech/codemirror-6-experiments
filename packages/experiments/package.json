    "@teamwork/websocket-json-stream": "^1.1.1",
    "codemirror-ot": "^1.3.0",
    "codemirror-theme-ubuntu": "1",
    "events": "^3.0.0",
    "express": "^4.16.4",
    "jsdom": "^13.2.0",
    "preact": "^8.4.2",
    "reconnecting-websocket": "^4.1.10",
    "sharedb": "1.0.0-beta.19",
    "ws": "^6.1.4"
  }
